#include <stdio.h>

  main()
 {
   int N;
   printf("ingresa un numero entre 0 > 5 para obtener tus resultados \n");
   scanf("%d", &N);
   switch (N)
   {
       case 0:
       printf("\n resultados 0 y 2."); break;
       case 1:
       printf("\n resultados 3, 5, 7 y 9. "); break;
       case 2:
       printf("\n resultados 6, 8, 10, 12, 14 y 16. "); break;
       case 3:
       printf("\n resultados 9, 11, 13, 15, 17, 19, 21 y 23. "); break;
       case 4:
       printf("\n resultados 12, 14, 16, 18, 20, 22, 24, 26, 28 y 30."); break;
       case 5:
       printf("\n resultados 15, 17, 19, 21, 23, 25, 27, 29, 31, 33 y 35. "); break;

       default:
       if (N>5)
       {
           printf("\nNumero incorecto. ");
       }
       else
       {
           printf("\nValor incorecto ");
       }
   }
 }
